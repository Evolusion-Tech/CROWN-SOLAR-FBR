<odoo>
    <!-- Tree View -->
    <record id="view_fbr_config_tree_only" model="ir.ui.view">
        <field name="name">fbr.config.tree.only</field>
        <field name="model">fbr.config</field>
        <field name="arch" type="xml">
            <tree string="FBR Config">
                <field name="token"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_fbr_config_form" model="ir.ui.view">
        <field name="name">fbr.config.form</field>
        <field name="model">fbr.config</field>
        <field name="arch" type="xml">
            <form string="FBR Config Form">
                <sheet>
                    <group>
                        <field name="token"/>
                        <field name="state" widget="radio"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Window Action: list + form -->
    <record id="action_fbr_config" model="ir.actions.act_window">
        <field name="name">FBR Configuration</field>
        <field name="res_model">fbr.config</field>
        <field name="view_mode">tree,form</field>
        <field name="target">current</field>
    </record>

       <record id="view_fbr_scenario_tree_only" model="ir.ui.view">
        <field name="name">fbr.scenario.tree.only</field>
        <field name="model">fbr.scenario</field>
        <field name="arch" type="xml">
            <tree string="FBR Scenario" editable="bottom">
                <field name="name"/>

            </tree>
        </field>
    </record>
    <record id="view_fbr_buyer_type_tree_only" model="ir.ui.view">
        <field name="name">fbr.buyer.type.tree.only</field>
        <field name="model">buyer.type</field>
        <field name="arch" type="xml">
            <tree string="FBR Buyer Type" editable="bottom">
                <field name="name"/>

            </tree>
        </field>
    </record>

    <record id="action_fbr_scenario" model="ir.actions.act_window">
        <field name="name">FBR Scenario</field>
        <field name="res_model">fbr.scenario</field>
        <field name="view_mode">tree</field>
        <field name="target">current</field>
    </record>
    <record id="action_fbr_buyer_type" model="ir.actions.act_window">
        <field name="name">FBR Buyer Type</field>
        <field name="res_model">buyer.type</field>
        <field name="view_mode">tree</field>
        <field name="target">current</field>
    </record>

    <!-- Menu Item -->
    <menuitem id="menu_fbr_config_list"
              name="FBR Configurations"
              action="action_fbr_config"
              parent="fbr_integration_fields.menu_fbr_configuration_new"
              sequence="10"/>
</odoo>

