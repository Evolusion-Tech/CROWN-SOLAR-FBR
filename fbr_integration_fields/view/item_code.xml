<odoo>
           <record id="view_fbr_item_code_tree" model="ir.ui.view">
        <field name="name">fbr.item.code.tree</field>
        <field name="model">fbr.item.code</field>
        <field name="arch" type="xml">
            <tree string="Fbr Item Code" >
                <field name="hs_code"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

     <record id="action_fbr_item_code" model="ir.actions.act_window">
    <field name="name">FBR Item Code</field>
    <field name="res_model">fbr.item.code</field>
    <field name="view_mode">list</field>
    <field name="target">current</field>
</record>
     <menuitem id="menu_fbr_item_code"
              name="Fbr Item Code"
               action="action_fbr_item_code"
               parent="fbr_integration_fields.menu_fbr_configuration_new"
              sequence="20"/>
</odoo>