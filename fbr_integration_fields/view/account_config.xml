<odoo>
    <record id="view_tax_account_configuration_tree" model="ir.ui.view">
        <field name="name">tax.account.configuration.tree</field>
        <field name="model">account.tax.configuration.custom</field>
        <field name="arch" type="xml">
            <tree string="Tax Accounts Configurations" editable="bottom">
                <field name="tax_withheld_account_id"/>
                <field name="extra_tax_account_id"/>
                <field name="further_tax_account_id"/>
                <field name="fed_payable_account_id"/>
            </tree>
        </field>
    </record>


    <record id="action_tax_account_configuration" model="ir.actions.act_window">
        <field name="name">Tax Accounts Configuration</field>
        <field name="res_model">account.tax.configuration.custom</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Configure tax accounts mapping
            </p>
        </field>
    </record>

</odoo>